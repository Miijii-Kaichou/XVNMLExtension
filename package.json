{
    "name": "xvnmlext",
    "displayName": "XVNMLExt",
    "description": "Language Support for XVNML",
    "version": "0.1.121",
    "preview": true,
    "publisher": "Miijii",
    "repository": {
        "url": "https://github.com/Miijii-Kaichou/XVNML.git"
    },
    "author": {
        "name": "Miijii",
        "email": "miijii@enjyoii.co",
        "url": "https://www.enjyoii.co/"
    },
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xvnml",
            "aliases": ["XVNML", "xvnml"],
            "extensions": [".xvnml"],
