{
    "name": "xvnmlext",
    "displayName": "XVNMLExt",
    "description": "Language Support for XVNML",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xvnml",
            "aliases": ["XVNML", "xvnml"],
            "extensions": [".xvnml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xvnml",
            "scopeName": "source.xvnml",
            "path": "./syntaxes/xvnml.tmLanguage.json"
        }]
    }
}